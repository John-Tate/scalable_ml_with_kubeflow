{
    "name": "kubeflow-centraldashboard",
    "version": "0.0.2",
    "description": "Kubeflow Central Dashboard",
    "scripts": {
        "build": "cross-env NODE_ENV=production webpack && npm run tslint && npm run build-ts",
        "build-ts": "tsc",
        "build-webpack": "webpack",
        "debug": "npm run build && npm run watch-debug",
        "dev": "concurrently -k -p \"[{name}]\" -n \"Webpack,TypeScript,Express\" -c \"green.bold,yellow.bold,cyan.bold\" \"webpack-dev-server\" \"npm run watch-ts\" \"npm run watch-node\"",
        "dev-debug": "concurrently -k -p \"[{name}]\" -n \"Webpack,TypeScript,Express\" -c \"green.bold,yellow.bold,cyan.bold\" \"webpack-dev-server\" \"npm run watch-ts\" \"npm run serve-debug\"",
        "dev-server-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Express\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
        "serve": "node dist/server.js",
        "serve-debug": "nodemon --inspect dist/server.js",
        "start": "npm run serve",
        "test": "npm run test-client && npm run test-server",
        "test-client": "karma start --single-run",
        "test-client-debug": "karma start --browsers Chrome",
        "test-server": "ts-node --transpile-only node_modules/jasmine/bin/jasmine --config=app/.jasmine.json --reporter=jasmine-console-reporter",
        "tslint": "tslint -c tslint.json -p tsconfig.json",
        "coverage": "nyc npm run test-server && karma start --single-run --reporters coverage-istanbul",
        "watch-node": "nodemon dist/server.js",
        "watch-ts": "tsc -w"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kubeflow/kubeflow.git"
    },
    "author": "Kubeflow Project",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/kubeflow/kubeflow/issues"
    },
    "vaadin": {
        "disableUsageStatistics": true
    },
    "homepage": "https://github.com/kubeflow/kubeflow#readme",
    "dependencies": {
        "@babel/polyfill": "^7.4.4",
        "@google-cloud/monitoring": "^0.7.1",
        "@kubernetes/client-node": "^0.8.2",
        "@polymer/app-layout": "^3.0.0",
        "@polymer/app-route": "^3.0.0",
        "@polymer/iron-a11y-keys": "^3.0.1",
        "@polymer/iron-ajax": "^3.0.1",
        "@polymer/iron-collapse": "^3.0.1",
        "@polymer/iron-flex-layout": "^3.0.0",
        "@polymer/iron-icons": "^3.0.1",
        "@polymer/iron-iconset-svg": "^3.0.0",
        "@polymer/iron-media-query": "^3.0.1",
        "@polymer/iron-pages": "^3.0.0",
        "@polymer/iron-selector": "^3.0.0",
        "@polymer/neon-animation": "^3.0.1",
        "@polymer/paper-button": "^3.0.1",
        "@polymer/paper-card": "^3.0.1",
        "@polymer/paper-dropdown-menu": "^3.0.1",
        "@polymer/paper-icon-button": "^3.0.0",
        "@polymer/paper-item": "^3.0.1",
        "@polymer/paper-listbox": "^3.0.1",
        "@polymer/paper-menu-button": "^3.0.1",
        "@polymer/paper-progress": "^3.0.1",
        "@polymer/paper-ripple": "^3.0.1",
        "@polymer/paper-tabs": "^3.1.0",
        "@polymer/paper-toast": "^3.0.1",
        "@polymer/polymer": "^3.2.0",
        "@types/dotenv": "^6.1.1",
        "@vaadin/vaadin-grid": "^5.4.6",
        "@webcomponents/webcomponentsjs": "^2.2.10",
        "chart.js": "^2.8.0",
        "chartjs-plugin-crosshair": "^1.1.2",
        "express": "^4.16.4",
        "web-animations-js": "^2.3.1"
    },
    "devDependencies": {
        "@babel/core": "^7.4.4",
        "@babel/plugin-transform-runtime": "^7.4.4",
        "@babel/preset-env": "^7.4.4",
        "@babel/runtime": "^7.4.4",
        "@polymer/test-fixture": "^4.0.2",
        "@types/express": "^4.16.1",
        "@types/gapi.client.monitoring": "^3.0.1",
        "@types/jasmine": "^3.3.12",
        "babel-loader": "^8.0.5",
        "clean-webpack-plugin": "^1.0.1",
        "concurrently": "^4.1.0",
        "copy-webpack-plugin": "^5.0.3",
        "core-js": "^2.6.5",
        "cross-env": "^5.2.0",
        "css-loader": "^2.1.1",
        "eslint": "^5.16.0",
